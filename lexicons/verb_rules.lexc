! Verb-prefixes -> Tense-pref -> Verbs -> V -> IV/TV -> V-aspect -> V-tense ->
! -> Thematic???Where??? -> Subj/Agent ( -> Object )


!!!!!! Subject/agent verbal prefixes

LEXICON Verb-prefixes
@P.SUBJ.t@:@P.SUBJ.t@т%{ы%}%> V-TENSE-PREFIXES ;
@P.SUBJ.myt@:@P.SUBJ.myt@мыт%{ы%}%> V-TENSE-PREFIXES ;
@P.SUBJ.m@:@P.SUBJ.m@м%{ы%}%> V-TENSE-PREFIXES ;
@P.SUBJ.myn@:@P.SUBJ.myn@мын%{ы%}%> V-TENSE-PREFIXES ;
@P.SUBJ.q@:@P.SUBJ.q@ӄ%{ы%}%> V-TENSE-PREFIXES ;
@P.SUBJ.n@:@P.SUBJ.n@н%{ы%}%> V-TENSE-PREFIXES ;


!!!!!! Tense prefixes (fut)

LEXICON V-TENSE-PREFIXES
@P.TENSE.fut@:@P.TENSE.fut@рэ%> Verbs ;
@P.TENSE.cond@:@P.TENSE.cond@ъ%> Verbs ;
: Verbs ;



LEXICON V
%<v%>%<iv%>: V-IV ;
! %<v%>%<tv%>: V-TV ;



!!!!!! Aspect & stative

LEXICON V-IV
: V-IV-ACTIVE ;
@D.TENSE@@D.SUBJ@%<stat%>:@D.TENSE@@D.SUBJ@ V-IV-STATIVE ;
@D.TENSE@@D.SUBJ@:@D.TENSE@@D.SUBJ@ V-IV-NONFIN ;

LEXICON V-IV-ACTIVE
%<neut%>: V-IV-NEUT ;
%<prog%>:%>%{ы%}ркы V-IV-PROG ;

LEXICON V-IV-STATIVE
%<perf%>: V-IV-PERFECT ;
%<hab%>: V-IV-HABITUAL ;

LEXICON V-IV-NONFIN
%<inf%>:%>%{ы%}к # ;
%<gna_sim%>:%>%{ы%}ма # ;
%<gna_ant%>:%>%{ы%}к # ;
%<gna_res%>:%>%{и%}нэӈу # ;


! LEXICON V-TV



LEXICON V-IV-PERFECT
%<s_sg1%>:%>%{I%}гым # ;
%<s_pl1%>:%>%{ы%}мури # ;
%<s_sg2%>:%>%{I%}гыт # ;
%<s_pl2%>:%>%{ы%}тури # ;
%<s_sg3%>:%>%{ы%}лин # ;
%<s_pl3%>:%>%{ы%}линэт # ;

LEXICON V-IV-HABITUAL
%<s_sg1%>:%>%{I%}гым # ;
%<s_pl1%>:%>%{ы%}мури # ;
%<s_sg2%>:%>%{I%}гыт # ;
%<s_pl2%>:%>%{ы%}тури # ;
%<s_sg3%>:%>%{ы%}ӄин # ;
%<s_pl3%>:%>%{ы%}ӄинэт # ;


!!! Active

!!!!!! Tense

LEXICON V-IV-NEUT
@D.TENSE@%<aor%>:@D.TENSE@ V-IV-NEUT-AOR ;
@R.TENSE.fut@%<fut%>:@R.TENSE.fut@ V-IV-NEUT-FUT ;
! @D.TENSE@%<fut%>:@D.TENSE@ V-IV-NEUT-FUT ;
@D.TENSE@%<intn%>:@D.TENSE@ V-IV-NEUT-INTN ;
@R.TENSE.cond@%<cond%>:@R.TENSE.cond@ V-IV-NEUT-COND ;

LEXICON V-IV-PROG
@D.TENSE@%<aor%>:@D.TENSE@ V-IV-PROG-AOR ;
@R.TENSE.fut@%<fut%>:@R.TENSE.fut@ V-IV-PROG-FUT ;
! @D.TENSE@%<fut%>:@D.TENSE@ V-IV-PROG-FUT ;
@D.TENSE@%<intn%>:@D.TENSE@ V-IV-PROG-INTN ;
@R.TENSE.cond@%<cond%>:@R.TENSE.cond@ V-IV-PROG-COND ;



!!!!!! Subject suffixes

LEXICON V-IV-NEUT-AOR
@R.SUBJ.t@%<s_sg1%>:@R.SUBJ.t@%>%{ы%}к # ;
@R.SUBJ.myt@%<s_pl1%>:@R.SUBJ.myt@%>%{ы%}мык # ;
